[Unit]
Description=⌚️ RPM-OSTree component that anonymously tracks the approximate age of the system installation, no personal info sent
Documentation=man:rpm-ostree-countme.timer(8)
ConditionPathExists=/run/ostree-booted
Wants=network-online.target

[Timer]
OnCalendar=daily
Persistent=true
AccuracySec=6h

[Install]
WantedBy=timers.target

[Unit]
Description=⌚️ RPM-OSTree component for anonymously reporting system details to Fedora for statistics
Documentation=man:rpm-ostree-countme.timer(8)
ConditionPathExists=/run/ostree-booted
After=network-online.target
Wants=network-online.target

[Timer]
OnCalendar=daily
Persistent=true
AccuracySec=11h

[Install]
WantedBy=timers.target
